[user]
	name = Dave Rostron
	email = yastero@gmail.com
[color]
	diff = auto
	status = auto
[core]
	excludesfile = /Users/dave/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	co = checkout
	br = branch
[push]
	default = simple
[merge]
	tool = opendiff
[commit]
	template = /Users/dave/.stCommitMsg
[include]
    path = /Users/dave/.gitconfig_private
